<input type="hidden" id="fail" value="<%=@fail%>">
<script>
$(function(){
var check_fail = $( '#fail' ).val();
    if( check_fail == "1" ){
            alert( "이미 체크 하셨습니다." );
            history.go(-2);
    }
    if( check_fail == "2" ){
            alert( "메일 받기를 신청하였습니다" );
            history.go(-1);
    }
});
</script>